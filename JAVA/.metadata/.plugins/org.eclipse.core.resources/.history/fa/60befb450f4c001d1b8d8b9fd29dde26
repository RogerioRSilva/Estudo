package fundamentos;

public class OperadoresTernários {
	
	public static void main(String[] args) {
		
		/*
		 *  =========== Atribuição condicional ================
		 * */
	
		double media = 5.6;
		
		String resultado = media >= 7 ? "Aprovado":"Reprovado";
		
		System.out.println(resultado);
		
	}
}
