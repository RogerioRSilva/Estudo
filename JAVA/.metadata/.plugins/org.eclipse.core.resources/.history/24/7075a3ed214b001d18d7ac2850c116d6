package fundamentos;

public class NotacaoPonto {
	
	public static void main(String[] args) {
		
		double a = 2.3;
		String s = "Bom dia X";
		s.toUpperCase();
		System.out.println(s.toUpperCase());
		
		s = s.replace("X", ", Tudo Bem");
		
		System.out.println(s.replace("X", ", Tudo Bem"));
		
		System.out.println(s.concat("!!!"));
		
		String y = "Bom dia X".replace("X", "Rog");
		System.out.println(y);
	}
}
